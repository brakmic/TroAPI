{
  "name": "troapi",
  "version": "0.0.2",
  "description": "troisdorf city api",
  "main": "index.js",
  "scripts": {
    "api": "babel-node api.js",
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "typings": "typings",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "protractor": "protractor",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc typings coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "npm run clean:dist && webpack --config config/webpack.dev.js --progress --profile",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "npm run clean:dist && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:prod": "http-server dist --cors",
    "vsrun": "webpack-dev-server --config config/webpack.dev.js --inline --profile --colors --watch --hot --display-error-details --display-cached --content-base src/",
    "lint": "npm run tslint -- 'src/**/*.ts'",
    "test": "no test",
    "docs": "npm run typedoc -- --options typedoc.json --name 'vr-demo-1' --hideGenerator --exclude '*.d.ts' --exclude '*.spec.ts' --exclude '*themes*' src/app",
    "start": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "version": "npm run build",
    "rebuild:sass": "npm rebuild node-sass"
  },
  "keywords": [
    "api",
    "troisdorf",
    "city"
  ],
  "author": "Harris Brakmic",
  "license": "MIT",
  "dependencies": {
    "@angular/compiler": "^2.4.9",
    "@angular/core": "^2.4.9",
    "animate.css": "^3.5.2",
    "bootstrap": "^3.3.7",
    "bows": "^1.5.0",
    "core-js": "^2.4.1",
    "cuid": "^1.3.8",
    "domready": "^1.0.8",
    "elasticlunr": "^0.9.5",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "html-parse-stringify": "^1.0.2",
    "jquery": "^3.1.1",
    "normalize.css": "^5.0.0",
    "redux": "^3.6.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.2.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^1.3.2",
    "@types/jquery": "^2.0.41",
    "@types/lodash": "^4.14.55",
    "@types/node": "^7.0.8",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.28",
    "@types/webpack": "^2.2.11",
    "add-asset-html-webpack-plugin": "^1.0.2",
    "assets-webpack-plugin": "^3.5.1",
    "awesome-typescript-loader": "^3.1.2",
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "base64-js": "^1.2.0",
    "bootstrap-loader": "^2.0.0",
    "bootstrap-sass": "^3.3.7",
    "circular-json": "^0.3.1",
    "codelyzer": "^3.0.0-beta.3",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.27.3",
    "domready": "^1.0.8",
    "es6-promise": "^4.1.0",
    "exports-loader": "^0.6.4",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "fast-html-parser": "^1.0.1",
    "file-loader": "^0.10.1",
    "font-awesome-sass-loader": "^1.0.3",
    "fs": "0.0.2",
    "hapi": "^16.1.0",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.9.0",
    "imports-loader": "^0.7.1",
    "inert": "^4.1.0",
    "isomorphic-fetch": "^2.2.1",
    "js-base64": "^2.1.9",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "node-babel": "^0.1.2",
    "node-sass": "^4.5.0",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "script-loader": "^0.7.0",
    "source-map-loader": "^0.2.0",
    "style-loader": "^0.14.1",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "ts-node": "^2.1.0",
    "tslib": "^1.6.0",
    "tslint": "^4.5.1",
    "tslint-loader": "^3.4.3",
    "typedoc": "^0.5.7",
    "typescript": "^2.2.1",
    "typescript-collections": "^1.2.3",
    "url-loader": "^0.5.8",
    "v8-lazy-parse-webpack-plugin": "^0.3.0",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^4.0.0",
    "whatwg-fetch": "^2.0.3"
  }
}
